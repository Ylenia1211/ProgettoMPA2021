<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="pane_main_grid" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #163754;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportAddDataController">

   <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Diagnosi" textFill="WHITE">
      <font>
         <Font name="Calibri" size="18.0" />
      </font>
   </Label>
   <TextArea fx:id="textDiagnosi" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" wrapText="true">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <font>
         <Font name="Calibri" size="13.0" />
      </font>
   </TextArea>
   <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Terapia" textFill="WHITE">
      <font>
         <Font name="Calibri" size="18.0" />
      </font>
   </Label>
   <TextArea fx:id="textTerapia" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" wrapText="true">
      <font>
         <Font name="Calibri" size="13.0" />
      </font></TextArea>
   <HBox fx:id="lastHbox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0">
      <TextField fx:id="textPath" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="306.0" />
      <ImageView fx:id="attachmentImage" fitHeight="20.0" fitWidth="19.0" onMouseClicked="#findAttachment" style="-fx-background-image: url('/resources/attachment.png');">
         <Image url="@../resources/Photo_Add-RoundedBlack-512.png" />
      </ImageView>
      <VBox fx:id="allegati" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Allegati:" textFill="WHITE">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Label>
         <Label fx:id="firstAttachment" alignment="CENTER" prefWidth="350" text="Nessuno" textAlignment="CENTER" textFill="WHITE">
            <font>
               <Font name="Calibri" size="10.0" />
            </font></Label>
      </VBox>
   </HBox>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" />
   </padding>
  <!-- <Button fx:id="creaReportButton" alignment="BOTTOM_CENTER" mnemonicParsing="false" text="Crea report">
      <font>
         <Font name="Arial" size="13.0" />
      </font>
   </Button> -->
 <!-- <Button fx:id="btnSaveReport" mnemonicParsing="false" text="Salva " onAction="#registrationReport" /> -->
</VBox>
